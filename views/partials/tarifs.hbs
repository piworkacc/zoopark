<section class="tarif" id="tarif">
    <h2 class="our-tarifs">Наши тарифы</h2>
    <div class="tarif-container">
        <div class="tarif__title">
            <h4 style="text-align:center; padding-top: 10px"> Категория билета </h4>
        <ul class="tarif__title-list">
            {{#each tarif}}
                <li class="tarif__item">{{PeopleType.name}}</li>
            {{/each}}
        </ul>
        </div>
        <div class="tarif__title">
            <h4 style="text-align:center; padding-top:10px"> Цена </h4>
            <ul class="tarif__title-list">
            {{#each tarif}}
                    <li class="tarif__item">{{value}}</li>
            {{/each}}
            </ul>
        </div>
    </div>
</section>
